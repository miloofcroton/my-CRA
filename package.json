{
  "name": "client",
  "version": "0.1.0",
  "description": "CRA starter",
  "keywords": [
    "Typescript",
    "React",
    "styled-components"
  ],
  "license": "MIT",
  "author": "Jack Toumey",
  "homepage": "https://github.com/miloofcroton/my-CRA#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/miloofcroton/my-CRA.git"
  },
  "bugs": {
    "url": "https://github.com/miloofcroton/my-CRA/issues"
  },
  "engines": {
    "node": ">= 11.10.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/react": "^5.2.0",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.4",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/styled-components": "^4.1.19",
    "awesome-typescript-loader": "^5.2.1",
    "customize-cra": "^0.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "react-app-rewired": "^2.1.3",
    "react-docgen-typescript-loader": "^3.2.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-test-renderer": "^16.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "typedoc": "^0.15.0"
  },
  "dependencies": {
    "@airbnb/lunar": "^2.27.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "styled-components": "^4.3.2",
    "typescript": "3.6.3"
  },
  "scripts": {
    "test": "react-app-rewired test --watchAll=false --coverage",
    "test:watch": "react-app-rewired test",
    "test:update": "react-app-rewired test --watchAll=false --updateSnapshot",
    "start": "serve -s build",
    "start:watch": "react-app-rewired start",
    "build": "react-app-rewired build",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "storybook:start": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o .storybook-static",
    "docs:markdown": "typedoc --theme markdown --out ./docs",
    "docs:raw": "typedoc --out ./docs",
    "docs:clean": "rimraf ./docs"
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  }
}
